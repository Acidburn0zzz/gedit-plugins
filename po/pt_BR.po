msgstr "Cria um arquivo diff apartir de dois documentos ou arquivos"

#. Create the dialog
#: plugins/diff/diff.c:270 plugins/diff/diff.gedit-plugin.desktop.in.h:1
msgid "Compare Files"
msgstr "Comparar Arquivos"

#: plugins/diff/diff.c:284
msgid "C_ompare"
msgstr "C_omparar"

#: plugins/diff/diff.c:511
msgid "Executed command"
msgstr "Executando o comando: %s"

#: plugins/diff/diff.c:652
msgid "The two documents you selected are the same."
msgstr "Os documentos selecionados são o mesmo."

#: plugins/diff/diff.c:660
msgid ""
"The \"first\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"O primeiro arquivo selecionado não existe.\n"
"\n"
"Digite um nome de arquivo válido."

#: plugins/diff/diff.c:669
msgid ""
"The \"second\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"O segundo arquivo selecionado não existe.\n"
"\n"
"Digite um nome de arquivo válido."

#: plugins/diff/diff.c:677
msgid "The two files you selected are the same."
msgstr "Os dois arquivos que você selecionou são o mesmo."

#: plugins/diff/diff.c:694
msgid "The \"first\" document contains no text."
msgstr "O primeiro documento não contem texto."

#: plugins/diff/diff.c:728
msgid "The \"second\" document contains no text."
msgstr "O segundo documento não contém texto."

#. FIXME: do better error reporting ... . Chema
#: plugins/diff/diff.c:754
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"gedit could not create a temporary file."
msgstr ""
"Impossível comparar os documentos selecionados.\n"
"\n"
"O gedit não pôde criar um arquivo temporário."

#: plugins/diff/diff.c:778 plugins/diff/diff.c:791
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"Error executing the diff command."
msgstr ""
"Impossível comparar os documentos selecionados.\n"
"\n"
"Erro ao executar o comando diff."

#: plugins/diff/diff.c:813
msgid "No differences were found between the selected documents."
msgstr "Não foram encontradas diferenças entre os documentos selecionados."

#: plugins/diff/diff.c:854
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"The result contains invalid UTF-8 data."
msgstr ""
"Impossível comparar os documentos selecionados.\n"
"\n"
"O resultado contém dados UTF-8 inválidos."

